{
    "_id": "$objectid",
    "tracking_number": {
        "$number": {
            "min": 10000000000000,
            "max": 99999999999999
        }
    },
    "datetime": "$date",
    "location": "$address",
    "gps": "$coordinates",
    "activity": {
        "$choose": {
                "from": [
                    "origin_scan",
                    "departure_scan",
                    "export_scan",
                    "customs_entry",
                    "customs_exit",
                    "import_scan",
                    "destination_scan",
                    "out_on_delivery",
                    "delivered"
                ]
            }
    },
    "internal_activity": "$bool"
}